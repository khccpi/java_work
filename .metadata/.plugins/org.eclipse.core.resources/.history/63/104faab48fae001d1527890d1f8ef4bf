
public class Identifier {

	public static void main(String[] args) {

		//꼭 지켜야 할 규칙 (안지킬 시 에러가 발생할 수 있습니다.)
		//1. 식별자의 이름은 중복을 허용하지 않습니다.
		//2. 대소문자를 철저하게 구분합니다.
		
		int age = 35;
		//int age = 40; (X, 중복 불가)
		int Age = 40;
		System.out.println(age);
		System.out.println(Age);
		
		//3. 식별자 이름은 숫자로만 지정하거나 숫자로 시작하면 안됩니다.
		//int 700 = 365; (X, 변수 잘못 지정)
	
	}

}
